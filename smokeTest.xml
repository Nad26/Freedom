<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke" parallel="tests" >

	<test name="SmokeTest_chrome" group-by-instances="true" preserve-order="true" >
		<parameter name="browser" value="chrome"/>
		<classes>
			<class name="runner.MainRunner"></class>
		</classes>
	</test>
	<test name="SmokeTest_ff" group-by-instances="true" preserve-order="true" >
		<parameter name="browser" value="firefox"/>
		<classes>
			<class name="runner.MainRunner"></class>
		</classes>
	</test>
</suite>
